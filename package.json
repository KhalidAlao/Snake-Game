{
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.1",
    "@eslint/js": "^9.36.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2"
  },
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
